<% if (locals.loggedin && locals.accountData) { %>
<h2>Welcome <%= locals.accountData.account_firstname %></h2>
<a
  href="/account/update/<%= locals.accountData.account_id %>"
  >Update Account Information</a
>
<% if (locals.accountData.account_type === 'Admin' ||
locals.accountData.account_type === 'Employee') { %>
<h3>Inventory Management</h3>
<a href="/inv">Manage Inventory</a>
<% } %> <% } else { %>
<p>Please log in to access this page.</p>
<% } %>
